    <script>
        function TestExam() {
            var answers = [];
            for (var i = 1; i <= 20; i++) {
                var nameAnswere = "#answers" + i;
                var conceptName = $(nameAnswere).find(":selected").val();
                answers.push(conceptName)
            }
            var minhnha = {
                Answers: answers,
            }
            $.ajax({
                type: "POST",
                contentType: "application/json",
                url: "/Authentication/TestExam",
                data: JSON.stringify(minhnha),
                dataType: 'json',
                cache: false,
                success: function (t) {
                    if (t.Status == true) {
                        $("#point").text(t.Total);
                        $('#exampleModalAddO').modal('show');
                    } else{
                        swal({
                            title: 'Cần chọn đầy đủ đáp án!',
                            /* text: 'Redirecting...', */
                            icon: 'error',
                            timer: 3000,
                            buttons: true,
                            type: 'error'
                        })
                    }
                },
                error: function (e) {
                    alert(e)
                }
            });          
        }
    </script>
